{
    "builds": [{
        "src": "controller/controller/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.11"}
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/controller/controller/wsgi.py"
        }
    ]
}